// 跳转地址
function jumpToUrl(url, name, anchor) {
if (!url || !name) {
return;
}
// 新老地址切换
var oldPage = {
'financen': true
}
var pageLocation = window.localStorage.getItem('memoryPage');
// 如果不是客户端且客户端内核是IE
if (isClientIE() || (pageLocation && pageLocation == 'old' && oldPage[name])) {
location.href = url;
return;
}
var code = $('#stockCode').val();
